Public Class MSEButton

#Region "コンストラクタ"

    Public Sub New()

        Me.Font = My.Settings.ButtonFont
        Me.BackColor = My.Settings.ButtonColor
        Me.ForeColor = Color.WhiteSmoke
        Me.FlatStyle = FlatStyle.Standard
        Me.UseVisualStyleBackColor = False


    End Sub

#End Region

End Class
